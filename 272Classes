   }
   
   public  class Admin
{
  //data members
  private int mAdminID;
  private string mAdminName;
  private string mAdminLastName;

  //constructor method
  public Material()
   {
     mAdminID = "0";
     mAdminName = "";
     mAdminLastName = "";
   }

   //Properties
   public int AdminID
   {
     get { return mAdminID; }
     set { mAdminID = value: }
   }
      public int AdminName
   {
     get { return mAdminName; }
     set { mAdminName = value: }
   }
      public int AdminLAstName
   {
     get { return mAdminLastName; }
     set { mAdminLastName = value: }
   }
   
