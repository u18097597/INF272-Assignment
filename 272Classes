public  class Material
{
  //data members
  private int mMaterialTypeID;
  private string mName;

  //constructor method
  public Material()
   {
     mMaterialTypeID = "0";
     mName = "";
   }

   //Properties
   public int MaterialTypeID
   {
     get { return mMaterialTypeID; }
     set { mMaterialTypeID = value: }
   }
      public int Name
   {
     get { return mName; }
     set { mName = value: }
   }

public  class MaterialType
{
  //data members
  private int mMaterialTypeID;
  private string mTypeName;

  //constructor method
  public MaterialType()
   {
     mMaterialTypeID = "0";
     mTypeName = "";
   }

   //Properties
   public int MaterialTypeID
   {
     get { return mMaterialTypeID; }
     set { mMaterialTypeID = value: }
   }
      public int TypeName
   {
     get { return mTypeName; }
     set { mTypeName = value: }
   }
   
   public  class Subject
{
  //data members
